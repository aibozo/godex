# LLM Provider API Keys
# At least one of these is required

# OpenAI API Key (for GPT models)
OPENAI_API_KEY=your-openai-key-here

# Anthropic API Key (for Claude models)
ANTHROPIC_API_KEY=your-anthropic-key-here

# Google Gemini API Key (for Gemini models)
GEMINI_API_KEY=your-gemini-key-here

# Optional: Daily cost cap in USD (default: 20.0)
# COST_CAP_DAILY=20.0

# Optional: Default model (default: gemini-2.5-flash-preview-05-20)
# MODEL_ROUTER_DEFAULT=gemini-2.5-flash-preview-05-20

# Optional: Enable Gemini thinking mode (default: false)
# GEMINI_THINKING_MODE=false

# Optional: Daily token budget (legacy setting)
# DAILY_TOKEN_BUDGET=100000